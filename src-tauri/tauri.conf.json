{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Hoyland3-AlteredStates",
    "version": "0.0.1"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "windows": [
      {
        "title": "HoylandGenerator3",
        "width": 1200,
        "height": 700,
        "alwaysOnTop": false
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": ["nsis"],
      "identifier": "com.hoyland.asl",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": "",
        "wix": {
          "language": "en-US"
        },
        "webviewInstallMode": {
          "type": "embedBootstrapper",
          "silent": true
        },
        "nsis": {
          "installerIcon": "icons/icon.ico",
          "script": "./custom.nsi"
        }
      }
    }
  }
}
